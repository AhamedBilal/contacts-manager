{
  "name": "contacts-manager",
  "version": "0.0.1",
  "main": "main.js",
  "author": "Ahamed Bilal",
  "description": "Contact managing App",
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "build": "electron-builder",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "ng build --prod",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "private": true,
  "dependencies": {
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0",
    "xlsx": "^0.16.9"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.3",
    "@angular/animations": "~11.0.3",
    "@angular/cdk": "^11.0.2",
    "@angular/cli": "~11.0.3",
    "@angular/common": "~11.0.3",
    "@angular/compiler": "~11.0.3",
    "@angular/compiler-cli": "~11.0.3",
    "@angular/core": "~11.0.3",
    "@angular/forms": "~11.0.3",
    "@angular/material": "^11.0.2",
    "@angular/platform-browser": "~11.0.3",
    "@angular/platform-browser-dynamic": "~11.0.3",
    "@angular/router": "~11.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "bootstrap": "^4.5.3",
    "codelyzer": "^6.0.0",
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-electron": "^2.2.0",
    "protractor": "~7.0.0",
    "rxjs": "~6.6.0",
    "ts-node": "~8.3.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "zone.js": "~0.10.2"
  },
  "build": {
    "appId": "com.bilal.contactmanager",
    "productName": "Conatct Manager App",
    "target": "NSIS",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "dist/contacts-manager/assets/logo.ico",
      "uninstallerIcon": "dist/contacts-manager/assets/logo.ico",
      "perMachine": true
    },
    "win": {
      "icon": "dist/contacts-manager/assets/logo.ico"
    }
  }
}
